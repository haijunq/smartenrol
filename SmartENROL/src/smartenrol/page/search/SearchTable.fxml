<?xml version="1.0" encoding="UTF-8"?>

<?import smartenrol.page.search.*?>
<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<TableView fx:id="view" prefHeight="200.0" prefWidth="200.0"             
           xmlns:fx="http://javafx.com/fxml" 
           fx:controller="smartenrol.page.search.SearchTableController">
  <columns>
    <TableColumn prefWidth="75.0" text="Course ID">
        <cellValueFactory>
          <PropertyValueFactory property="courseID" />
        </cellValueFactory>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Course Name">
        <cellValueFactory>
          <PropertyValueFactory property="courseName" />
        </cellValueFactory>
    </TableColumn>    
    <TableColumn prefWidth="75.0" text="Program" >
        <cellValueFactory>
          <PropertyValueFactory property="program" />
        </cellValueFactory>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Credits">
        <cellValueFactory>
          <PropertyValueFactory property="credits" />
        </cellValueFactory>
    </TableColumn>
  </columns>
</TableView>
