<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet ../../css/se-styles.css?>

<BorderPane id="BorderPane" fx:id="view" prefHeight="500.0" prefWidth="900.0" 
            xmlns:fx="http://javafx.com/fxml" 
            fx:controller="smartenrol.page.search.SearchController">
  <center>
    <BorderPane fx:id="innerContent" prefHeight="200.0" prefWidth="200.0">
      <center>
        <BorderPane fx:id="searchResultsArea" prefHeight="200.0" prefWidth="200.0" />
      </center>
      <left>
        <BorderPane fx:id="filterSidebar" prefHeight="200.0" prefWidth="200.0" />
      </left>
    </BorderPane>
  </center>
  <stylesheets>
    <URL value="@../css/SmartENROL.css" />
  </stylesheets>
  <top>
    <Pane prefHeight="59.0" prefWidth="900.0" styleClass="blue">
      <children>
        <TextField fx:id="mainSearchField" layoutX="103.0" layoutY="14.0" prefHeight="27.9609375" prefWidth="226.0" promptText="Universal Search" />
        <ImageView fitHeight="23.0" fitWidth="28.999998865900814" layoutX="338.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" styleClass="whiteBorder">
          <image>
            <Image url="@../../images/se-search-btn.png" preserveRatio="false" smooth="false" />
          </image>
        </ImageView>
        <Label layoutX="30.0" layoutY="14.0" text="Search" textFill="WHITE">
          <font>
            <Font name="Myanmar Text" size="20.0" />
          </font>
        </Label>
      </children>
    </Pane>
  </top>
</BorderPane>
